Private Sub cmdthrow_Click()
    Dim counter As Integer
    tcpclient.SendData "cardthrown|" & My_cards(cardpointer).id
    imgthrown(1).Picture = faceimg(cardpointer).Picture
    faceimg(cardpointer).Top = 11160
    faceimg(cardpointer).Visible = False
    imgthrown(1).Visible = True
    cmdthrow.Enabled = False
    For counter = 1 To 13 Step 1
        If faceimg(counter).Visible = True Then
            If counter = cardpointer Then
                'do nothing
            Else
                faceimg(counter).Top = 11160
            End If
        End If
    Next counter
    sel1 = False
    sel2 = False
    sel3 = False
    highest = 0
    throwflag = True
End Sub
Private Sub faceimg_Click(Index As Integer)
    cardpointer = Index
    cmdthrow.Enabled = True
