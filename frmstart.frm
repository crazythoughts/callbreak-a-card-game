'================================================================DATA ARRIVAL===========================================================================>
Private Sub tcpclient_DataArrival(ByVal bytesTotal As Long)
    Dim rec_card_id(13) As Integer
    Dim totalmsg() As String
    Dim counter_first As Integer
    Dim counter_second As Integer
    Dim counter_third As Integer
    sel1 = False
    sel2 = False
    sel3 = False
    tcpclient.GetData data_recieved
    totalmsg() = Split(data_recieved, "|")
    If totalmsg(0) = "message" Then
        txtdispchat.Text = txtdispchat.Text & totalmsg(1) & "::" & totalmsg(2) & vbNewLine
    ElseIf totalmsg(0) = "playerinfo" Then
        Players(1).id = CInt(totalmsg(1))
        Players(1).player_name = totalmsg(2)
        lblnamec(1).Caption = Players(1).player_name
        lblnamet(1).Caption = Players(1).player_name
        Players(1).ccall = CInt(totalmsg(3))
        Players(1).tpoints = CDbl(totalmsg(4))
        Players(1).cpoints = CDbl(totalmsg(5))
        Players(1).win = CInt(totalmsg(6))
        Players(2).id = CInt(totalmsg(7))
        Players(2).player_name = totalmsg(8)
        lblnamec(2).Caption = Players(2).player_name
        lblnamet(2).Caption = Players(2).player_name
        Players(2).ccall = CInt(totalmsg(9))
        Players(2).tpoints = CDbl(totalmsg(10))
        Players(2).cpoints = CDbl(totalmsg(11))
        Players(2).win = CInt(totalmsg(12))
        Players(3).id = CInt(totalmsg(13))
        Players(3).player_name = totalmsg(14)
        lblnamec(3).Caption = Players(3).player_name
        lblnamet(3).Caption = Players(3).player_name
        Players(3).ccall = CInt(totalmsg(15))
        Players(3).tpoints = CDbl(totalmsg(16))
        Players(3).cpoints = CDbl(totalmsg(17))
        Players(3).win = CInt(totalmsg(18))
        Players(4).id = CInt(totalmsg(19))
        Players(4).player_name = totalmsg(20)
        lblnamec(4).Caption = Players(4).player_name
        lblnamet(4).Caption = Players(4).player_name
        Players(4).ccall = CInt(totalmsg(21))
        Players(4).tpoints = CDbl(totalmsg(22))
        Players(4).cpoints = CDbl(totalmsg(23))
        Players(4).win = CInt(totalmsg(24))
        For counter_first = 1 To 4 Step 1
            If Players(counter_first).player_name = player_name Then
                Dim i As Integer
                For i = 1 To 4
                    lbldsp(i).Caption = Players(counter_first).player_name & "(" & Players(counter_first).win & ")"
                    counter_first = counter_first + 1
                    If counter_first > 4 Then
                        counter_first = 1
                    End If
                Next i
                Exit For
            End If
        Next counter_first
    ElseIf totalmsg(0) = "call" Then
        Players(CInt(totalmsg(1))).ccall = CInt(totalmsg(2))
        lblccall(CInt(totalmsg(1))) = Players(CInt(totalmsg(1))).ccall
        total = total + Players(CInt(totalmsg(1))).ccall
        lblctotal.Caption = total
        extrac = 13 - total
        lblcextra.Caption = extrac
    ElseIf totalmsg(0) = "distrib" Then
        For counter_first = 1 To 13
            rec_card_id(counter_first) = CInt(totalmsg(counter_first))
        Next counter_first
        For counter_first = 1 To 13 Step 1
            For counter_second = 1 To 52 Step 1
                If deck(counter_second).id = rec_card_id(counter_first) Then
                    My_cards(counter_first).id = deck(counter_second).id
                    My_cards(counter_first).name = deck(counter_second).name
                    My_cards(counter_first).cvalue = deck(counter_second).cvalue
                    My_cards(counter_first).img = deck(counter_second).img
                    Exit For
                End If
            Next counter_second
        Next counter_first
        For counter_first = 1 To 13 Step 1
            faceimg(counter_first).Picture = My_cards(counter_first).img
            faceimg(counter_first).Visible = True
        Next counter_first
        Line1.Visible = True
        lblwonhands.Visible = True
        cmdthrow.Visible = True
        For counter_first = 0 To 12 Step 1
            topbackimg(counter_first).Visible = True
            rightbackimg(counter_first).Visible = True
            leftbackimg(counter_first).Visible = True
        Next counter_first
        frmcallcol.Show
    ElseIf totalmsg(0) = "GS" Then
        indexplus = 0
        total = 0
        extrac = 0
        For counter_first = 1 To 32 Step 1
            woncards(counter_first).Visible = False
        Next counter_first
        If totalmsg(1) = "ok" Then
            For counter_first = 1 To 13 Step 1
                faceimg(counter_first).Enabled = True
            Next counter_first
        Else
            For counter_first = 1 To 13 Step 1
                faceimg(counter_first).Enabled = False
            Next counter_first
        End If
    ElseIf totalmsg(0) = "oho" Then
        For counter_first = 1 To 4 Step 1
            If Players(counter_first).id = CInt(totalmsg(1)) Then
                If Players(counter_first).player_name = player_name Then
                    Players(counter_first).win = CInt(totalmsg(2))
                     won_cards(1).id = CInt(totalmsg(3))
                    won_cards(2).id = CInt(totalmsg(4))
                    won_cards(3).id = CInt(totalmsg(5))
                    won_cards(4).id = CInt(totalmsg(6))
                    For counter_second = 1 To 4 Step 1
                        For counter_third = 1 To 52 Step 1
                            If deck(counter_third).id = won_cards(counter_second).id Then
                                won_cards(counter_second).img = deck(counter_third).img
                                indexplus = indexplus + 1
                                woncards(indexplus).Picture = won_cards(counter_second).img
                                woncards(indexplus).Visible = True
                            End If
                        Next counter_third
                     Next counter_second
                Else
                    Players(counter_first).win = CInt(totalmsg(2))
                End If
                For counter_second = 1 To 4 Step 1
                    If lbldsp(counter_second).Caption = Players(counter_first).player_name & "(" & (Players(counter_first).win - 1) & ")" Then
                        lbldsp(counter_second).Caption = Players(counter_first).player_name & "(" & Players(counter_first).win & ")"
                        Exit For
                    End If
                Next counter_second
                Exit For
            End If
        Next counter_first
        If throw_count = 3 Then
            throw_count = 0
            throwflag = False
            flag = False
            flag1 = False
            first = ""
            second = ""
            highest = 0
            highest1 = 0
        End If
        For counter_first = 1 To 4 Step 1
            imgthrown(counter_first).Visible = False
        Next counter_first
    ElseIf totalmsg(0) = "RS" Then
    If totalmsg(1) = "ok" Then
        If flag = True And throwflag = False Then
            If flag1 = False Then
                For counter_third = 1 To 13 Step 1
                    If My_cards(counter_third).name = first And My_cards(counter_third).cvalue > highest Then
                        If faceimg(counter_third).Visible = True Then
                            faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                            faceimg(counter_third).Enabled = True
                            sel1 = True
                        End If
                    End If
                Next counter_third
                If sel1 = False Then
                    For counter_third = 1 To 13 Step 1
                        If My_cards(counter_third).name = first Then
                            If faceimg(counter_third).Visible = True Then
                                faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                faceimg(counter_third).Enabled = True
                                sel2 = True
                            End If
                        End If
                    Next counter_third
                End If
                If sel1 = False And sel2 = False Then
                    For counter_third = 1 To 13 Step 1
                        If My_cards(counter_third).name = "spade" Then
                            If faceimg(counter_third).Visible = True Then
                                faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                faceimg(counter_third).Enabled = True
                                sel3 = True
                            End If
                        End If
                    Next counter_third
                End If
                If sel1 = False And sel2 = False And sel3 = False Then
                    For counter_third = 1 To 13 Step 1
                        If faceimg(counter_third).Visible = True Then
                            faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                            faceimg(counter_third).Enabled = True
                        End If
                    Next counter_third
                End If
            Else
                If second = "spade" Then
                    For counter_third = 1 To 13 Step 1
                        If My_cards(counter_third).name = first Then
                            If faceimg(counter_third).Visible = True Then
                                faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                faceimg(counter_third).Enabled = True
                                sel3 = True
                            End If
                        End If
                    Next counter_third
                    If sel3 = False Then
                        For counter_third = 1 To 13 Step 1
                            If My_cards(counter_third).name = "spade" And My_cards(counter_third).cvalue > highest1 Then
                                If faceimg(counter_third).Visible = True Then
                                    faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                    faceimg(counter_third).Enabled = True
                                    sel2 = True
                                End If
                            End If
                        Next counter_third
                    End If
                    If sel3 = False And sel2 = False Then
                        For counter_third = 1 To 13 Step 1
                            If faceimg(counter_third).Visible = True Then
                                faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                faceimg(counter_third).Enabled = True
                            End If
                        Next counter_third
                    End If
                Else
                    For counter_third = 1 To 13 Step 1
                    If My_cards(counter_third).name = first And My_cards(counter_third).cvalue > highest Then
                        If faceimg(counter_third).Visible = True Then
                            faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                            faceimg(counter_third).Enabled = True
                            sel1 = True
                        End If
                    End If
                Next counter_third
                If sel1 = False Then
                    For counter_third = 1 To 13 Step 1
                        If My_cards(counter_third).name = first Then
                            If faceimg(counter_third).Visible = True Then
                                faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                faceimg(counter_third).Enabled = True
                                sel2 = True
                            End If
                        End If
                    Next counter_third
                End If
                If sel1 = False And sel2 = False Then
                    For counter_third = 1 To 13 Step 1
                        If My_cards(counter_third).name = "spade" Then
                            If faceimg(counter_third).Visible = True Then
                                faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                                faceimg(counter_third).Enabled = True
                                sel3 = True
                            End If
                        End If
                    Next counter_third
                End If
                If sel1 = False And sel2 = False And sel3 = False Then
                    For counter_third = 1 To 13 Step 1
                        If faceimg(counter_third).Visible = True Then
                            faceimg(counter_third).Top = faceimg(counter_third).Top - 200
                            faceimg(counter_third).Enabled = True
                        End If
                    Next counter_third
                End If
                End If
            End If
        Else
            For counter_first = 1 To 13 Step 1
                If faceimg(counter_first).Visible = True Then
                faceimg(counter_first).Top = faceimg(counter_first).Top - 200
                    faceimg(counter_first).Enabled = True
                End If
            Next counter_first
        End If
        Else
            For counter_first = 1 To 13 Step 1
                faceimg(counter_first).Enabled = False
            Next counter_first
        End If
    ElseIf totalmsg(0) = "eor" Then
        counter_second = 1
        For counter_first = 1 To 4 Step 1
            counter_second = counter_second + 1
            Players(counter_first).cpoints = Val(totalmsg(counter_second))
            counter_second = counter_second + 1
            Players(counter_first).tpoints = Val(totalmsg(counter_second))
        Next counter_first
        If CInt(totalmsg(1)) = 1 Then
            For counter_third = 1 To 4 Step 1
                lbltfr(counter_third).Caption = Players(counter_third).cpoints
                lblttotal(counter_third).Caption = Players(counter_third).tpoints
            Next counter_third
        ElseIf CInt(totalmsg(1)) = 2 Then
            For counter_third = 1 To 4 Step 1
                lbltsr(counter_third).Caption = Players(counter_third).cpoints
                lblttotal(counter_third).Caption = Players(counter_third).tpoints
            Next counter_third
        ElseIf CInt(totalmsg(1)) = 3 Then
            For counter_third = 1 To 4 Step 1
                lblttr(counter_third).Caption = Players(counter_third).cpoints
                lblttotal(counter_third).Caption = Players(counter_third).tpoints
            Next counter_third
        ElseIf CInt(totalmsg(1)) = 4 Then
            For counter_third = 1 To 4 Step 1
                lbltfrr(counter_third).Caption = Players(counter_third).cpoints
                lblttotal(counter_third).Caption = Players(counter_third).tpoints
            Next counter_third
        ElseIf CInt(totalmsg(1)) = 5 Then
            For counter_third = 1 To 4 Step 1
                lbltffr(counter_third).Caption = Players(counter_third).cpoints
                lblttotal(counter_third).Caption = Players(counter_third).tpoints
            Next counter_third
        End If
    ElseIf totalmsg(0) = "WG" Then
        For counter_first = 1 To 4 Step 1
            If Players(counter_first).id = Val(totalmsg(1)) Then
                winner_name = Players(counter_first).player_name
                frmwinner.Show
                Exit For
            End If
        Next counter_first
    ElseIf totalmsg(0) = "cardthrown" Then
        For counter_second = 1 To 52 Step 1
            If deck(counter_second).id = CInt(totalmsg(2)) Then
                If flag = False Then
                    first = deck(counter_second).name
                    flag = True
                End If
                If flag1 = False Then
                    If deck(counter_second).name <> first Then
                        flag1 = True
                        second = deck(counter_second).name
                        If second = "spade" Then
                            highest1 = deck(counter_second).cvalue
                        End If
                    End If
                End If
                If flag1 = True Then
                    If deck(counter_second).name = "spade" Then
                        If deck(counter_second).cvalue > highest1 Then
                            highest1 = deck(counter_second).cvalue
                        End If
                    End If
                End If
                For counter_first = 1 To 4 Step 1
                    If lbldsp(counter_first).Caption = Players(CInt(totalmsg(1))).player_name & "(" & Players(totalmsg(1)).win & ")" Then
                        If counter_first = 2 Then
                            rightbackimg(del_cards).Visible = False
                        ElseIf counter_first = 3 Then
                            topbackimg(del_cards).Visible = False
                        ElseIf counter_first = 4 Then
                            leftbackimg(del_cards).Visible = False
                        End If
                        imgthrown(counter_first).Picture = deck(counter_second).img
                        imgthrown(counter_first).Visible = True
                        If deck(counter_second).name = first Then
                            If deck(counter_second).cvalue > highest Then
                                highest = deck(counter_second).cvalue
                            End If
                        End If
                        throw_count = throw_count + 1
                        Exit For
                    End If
                    Next counter_first
                Exit For
            End If
        Next counter_second
    End If
End Sub
